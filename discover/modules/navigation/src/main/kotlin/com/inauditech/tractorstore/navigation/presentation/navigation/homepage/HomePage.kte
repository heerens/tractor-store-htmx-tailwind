<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Tractor Store</title>

    @template.navigation.head.common()
    @template.navigation.head.favicon()

</head>
<body data-boundary-page="explore">
@template.navigation.include.inculde(url = "/navigation/fragments/v1/header")

<div class="ts-container-px">
    <div class="flex gap-x-4 text-center mb-12">
        <a class="hover:underline" href="/products/classic">
            <img class="mb-2" src="https://blueprint.the-tractor.store/cdn/img/scene/500/classics.webp" srcset="https://blueprint.the-tractor.store/cdn/img/scene/500/classics.webp 500w, https://blueprint.the-tractor.store/cdn/img/scene/1000/classics.webp 1000w" sizes="100vw, (min-width: 500px) 50vw" alt="">
            Classic Tractors
        </a>
        <a class="hover:underline" href="/products/autonomous">
            <img class="mb-2" src="https://blueprint.the-tractor.store/cdn/img/scene/500/autonomous.webp" srcset="https://blueprint.the-tractor.store/cdn/img/scene/500/autonomous.webp 500w, https://blueprint.the-tractor.store/cdn/img/scene/1000/autonomous.webp 1000w" sizes="100vw, (min-width: 500px) 50vw" alt="">
            Autonomous Tractors
        </a>
    </div>
</div>

<div class="ts-container">
    @template.navigation.include.inculde(url = "/discovery/fragments/v1/recommendations/top")
</div>



@template.navigation.include.inculde(url = "/navigation/fragments/v1/footer")
</body>
</html>